@model IEnumerable<E_ComerceSystem.ApplicationUserModelView>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminNavBar.cshtml";
}


<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Image)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
         
        </td>
        <td>
            
            <label  name="Email">@item.Email </label>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
   
        <td>
            <img src="~/Content/Images/@item.Image" width="150px"/>
        </td>

        <td>
          
            <a href="~/Customer/Delete/@item.Id" name="id" >Delete</a>

        </td>
    </tr>
}

</table>
